{
  "name": "app-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:staging": "tsc -b && vite build --mode staging",
    "lint": "eslint .",
    "preview": "vite preview",
    "pages:dev": "npx wrangler pages dev dist --compatibility-date=2025-10-02 --proxy 3001 --port 8789",
    "pages:deploy": "npm run build && npx wrangler pages deploy dist --project-name=cospec-editor",
    "pages:deploy:staging": "npm run build:staging && npx wrangler pages deploy dist --project-name=cospec-staging"
  },
  "dependencies": {
